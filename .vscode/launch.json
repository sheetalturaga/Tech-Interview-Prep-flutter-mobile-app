{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Flutter",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart"
        },
        {
            "name": "Dart",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        

        {
            "name": "blindspot_app",
            "cwd": "blindspot_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "blindspot_app (profile mode)",
            "cwd": "blindspot_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "blindspot_app (release mode)",
            "cwd": "blindspot_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}